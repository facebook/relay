# %extensions%

interface Cat {
    description: String
    name: String
}

type Tabby implements Cat {
    description: String @relay_resolver(import_path: "TabbyResolver")
    name: String
    parent: Cat
}

type Persian implements Cat {
    description: String
    name: String
}

extend type Query {
    cat: Cat
}
