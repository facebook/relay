{
  "name": "relay",
  "description": "A framework for building data-driven React applications.",
  "version": "13.0.0-rc.0",
  "license": "MIT",
  "homepage": "https://relay.dev",
  "bugs": "https://github.com/facebook/relay/issues",
  "repository": "facebook/relay",
  "scripts": {
    "build": "gulp dist",
    "build:clean": "gulp cleanbuild",
    "build:watch": "gulp watch",
    "jest": "cross-env NODE_ENV=test jest \"$@\"",
    "lint": "eslint --max-warnings 0 .",
    "postinstall": "npm run build",
    "prepublishOnly": "npm run build:clean",
    "prettier": "find . -name node_modules -prune -or -name dist -prune -or -name '*.js' -print | xargs prettier --write",
    "test": "f() { EXIT=0; npm run typecheck || EXIT=$?; npm run test-dependencies || EXIT=$?; npm run jest \"$@\" || EXIT=$?; exit $EXIT; }; f",
    "test-dependencies": "node ./scripts/testDependencies.js",
    "typecheck": "flow check"
  },
  "dependencies": {
    "@babel/core": "^7.14.0",
    "@babel/generator": "^7.14.0",
    "@babel/parser": "^7.14.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-optional-catch-binding": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@babel/traverse": "^7.14.0",
    "@babel/types": "^7.0.0",
    "@jest/create-cache-key-function": "^26.6.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-macros": "^2.0.0",
    "babel-plugin-tester": "^6.0.1",
    "babel-preset-fbjs": "^3.4.0",
    "chalk": "^4.0.0",
    "cosmiconfig": "^5.0.5",
    "cross-env": "^7.0.2",
    "del": "6.0.0",
    "eslint": "5.4.0",
    "eslint-config-fbjs": "2.1.0",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-relay": "1.7.1",
    "fb-watchman": "^2.0.0",
    "fbjs": "^3.0.0",
    "flow-bin": "^0.165.0",
    "glob": "^7.1.1",
    "graphql": "15.3.0",
    "gulp": "4.0.2",
    "gulp-babel": "8.0.0",
    "gulp-chmod": "3.0.0",
    "gulp-header": "2.0.9",
    "gulp-rename": "^2.0.0",
    "gulp-util": "3.0.8",
    "immutable": "~3.7.6",
    "invariant": "^2.2.4",
    "jest": "^26.6.3",
    "nullthrows": "^1.1.1",
    "prettier": "^2.4.1",
    "promise-polyfill": "6.1.0",
    "react": "17.0.2",
    "react-refresh": "^0.9.0",
    "react-test-renderer": "17.0.2",
    "signedsource": "^1.0.0",
    "webpack": "^4.30.0",
    "webpack-stream": "^5.1.1",
    "yargs": "^15.3.1"
  },
  "private": true,
  "devEngines": {
    "node": ">=4.x",
    "npm": ">=2.x"
  },
  "prettier": {
    "bracketSameLine": true,
    "bracketSpacing": false,
    "parser": "flow",
    "requirePragma": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "testRegex": "/__tests__/.*-test\\.js$",
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/",
      "<rootDir>/node_modules/(?!(fbjs/lib/|react/lib/))"
    ],
    "rootDir": "",
    "roots": [
      "<rootDir>/node_modules/fbjs/lib/",
      "<rootDir>/packages/"
    ],
    "setupFiles": [
      "<rootDir>/scripts/jest/environment.js"
    ],
    "timers": "fake",
    "transform": {
      ".*": "<rootDir>/scripts/jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testEnvironment": "node"
  },
  "devDependencies": {}
}
