{
  "name": "relay-compiler",
  "description": "A compiler tool for building GraphQL-driven applications.",
  "version": "14.1.0",
  "keywords": [
    "graphql",
    "relay"
  ],
  "license": "MIT",
  "homepage": "https://relay.dev",
  "bugs": "https://github.com/facebook/relay/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/facebook/relay.git",
    "directory": "packages/relay-compiler"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "bin": "cli.js",
  "devDependencies": {
    "ts-json-schema-generator": "^1.2.0",
    "typescript-json-schema": "^0.55.0"
  },
  "scripts": {
    "create-json-schema-2": "ts-json-schema-generator --path ./index.d.ts --type CompilerConfig",
    "create-json-schema": "typescript-json-schema ./index.d.ts CompilerConfig -o ./config-schema.json"
  }
}
