//- relay.config.json
{
   "language": "typescript",
   "schema": "./schema.graphql",
   "src": "./src"
}

//- schema.graphql
type Query { _: ID }

//- full_schema.graphql
type Query {
  me: User
  other: User
  settings: Settings
}

type User {
  id: ID!
  name: String
  age: Int
  email: String
}

type Settings {
  theme: String
  notifications: Boolean
}
